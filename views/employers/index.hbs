<h1>{{title}}</h1>
<a href="/employers/create" class="btn btn-secondary">Add a new Employer</a>
<section class="row">
    {{#each employers}}
    <div class="card col 4">
        <div class="card-body">
            <h5 class="card-tittle">{{this.name}}</h5>
            <p class="card-text">{{this.location}}</p>
        </div>
    {{/each}}
</section>